{
  "name": "coding-agent",
  "version": "1.0.0",
  "description": "Autonomous coding agent with git, file operations, and command execution capabilities",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "tsx src/cli/index.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src"
  },
  "keywords": [
    "ai",
    "coding",
    "agent",
    "automation",
    "git",
    "files"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@octokit/rest": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "ai": "^4.0.0",
    "chokidar": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "simple-git": "^3.27.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
